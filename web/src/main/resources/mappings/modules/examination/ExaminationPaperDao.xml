<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jeesite.modules.examination.dao.ExaminationPaperDao">
	
	<!-- 查询数据
	<select id="findList" resultType="ExaminationPaper">
		SELECT ${sqlMap.column.toSql()}
		FROM ${sqlMap.table.toSql()}
		<where>
			${sqlMap.where.toSql()}
		</where>
		ORDER BY ${sqlMap.order.toSql()}
	</select> -->
	<select id="saveid" resultType="ExaminationPaper">
		insert into examination_paper (question_id,exam_name)
		value (question_id=#{ids},exam_name=#{examName})
	</select>
	<select id="listpapersin" resultType="SingleSelection">
		select *
		from single_selection
		where instr(#{ids,jdbcType=VARCHAR}, id)
	</select>
	<select id="listpapermul" resultType="MultipleSelection">
		select *
		from multiple_selection
		where instr(#{ids,jdbcType=VARCHAR}, id)
	</select>
</mapper>