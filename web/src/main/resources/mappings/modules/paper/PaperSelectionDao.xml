<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jeesite.modules.paper.dao.PaperSelectionDao">
	
	<!-- 查询数据
	<select id="findList" resultType="PaperSelection">
		SELECT ${sqlMap.column.toSql()}
		FROM ${sqlMap.table.toSql()}
		<where>
			${sqlMap.where.toSql()}
		</where>
		ORDER BY ${sqlMap.order.toSql()}
	</select> -->
	<select id="findpapersin" resultType="String">
		select sin_question_id
		from paper_selection
		where status=0 and paper_id=#{paper_id}
	</select>
	<select id="findpapermul" resultType="String">
		select mul_question_id
		from paper_selection
		where status=0 and paper_id=#{paper_id}
	</select>
